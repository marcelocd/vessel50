<tr>
  <td>
    <%= link_to vessel_path(tracking.vessel.id), class: 'list-thumb-link' do %>
      <%= image_tag(tracking.vessel.image_url) %>
    <% end %>
  </td>
  <td><%= tracking.vessel.name %></td>
  <td><%= tracking.vessel.vessel_type.name %></td>
  <td><%= tracking.vessel.imo %></td>
  <td><%= tracking.vessel.callsign %></td>
  <td><%= tracking.vessel.mmsi %></td>
  <td><%= tracking.vessel.dimensions %></td>
  <td><%= tracking.area %></td>
  <td><%= tracking.last_seen %></td>
</tr>
